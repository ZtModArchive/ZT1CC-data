[Global]
Class = guests
Type =
Subtype =
DefaultSubtype = man

[Characteristics/Integers]
; These are global too
cSearchRadius = 40			
; the following assumes a 0 to 100 scale
; if scale is -100 to 100 code will adjust appropriately			
cUIThreshold = 75
; cRandomStatNth is the frequency of guests affected by the randomizations below
; e.g. 3 means every 3nd guest is affected and 2 means every other guest is affected
; ONLY ONE STAT PER GUEST WILL BE RANDOMIZED!
cRandomStatNth = 2
; cRandomThirstMin is the minimum of the range used to randomize beginning thirst stat for guest
cRandomThirstMin = 41
; cRandomThirstMax is the maximum of the range used to randomize beginning thirst stat for guest
cRandomThirstMax = 50
; cRandomHungerMin is the minimum of the range used to randomize beginning hunger stat for guest
cRandomHungerMin = 41
; cRandomHungerMax is the maximum of the range used to randomize beginning hunger stat for guest
cRandomHungerMax = 50
; cRandomEnergyMin is the minimum of the range used to randomize beginning energy stat for guest
cRandomEnergyMin = 0
; cRandomEnergyMax is the maximum of the range used to randomize beginning energy stat for guest
cRandomEnergyMax = 0
; cRandomBathroomMin is the minimum of the range used to randomize beginning energy stat for guest
cRandomBathroomMin = 0
; cRandomBathroomMax is the maximum of the range used to randomize beginning energy stat for guest
cRandomBathroomMax = 0
; shadows for this unit
cUsesRealShadows = 1
cHasShadowImages = 1
cHasUnderwaterSection = 1

