[f\BehaviorSet\bcgsurf]
f = fPlayTime(surfidle,4)

[f\BehaviorSet\bcgsub]
f = fPlayTime(subidle,4)

[f\BehaviorSet\bRunFromPredator]
f = fRun(0,0)

[f\BehaviorSet\bCaughtPrey]
f = fDustBall()
