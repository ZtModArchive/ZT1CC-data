;mermaid surface behavior sets
[f\BehaviorSet\bSurfswim]
f = fWalk(surface, 0)

[f\BehaviorSet\bSurfdive]
f = fWalk(dive, 0)

[f\BehaviorSet\bSurfloor]
f = fWalk(floor, 0)

[f\BehaviorSet\bSurfrest]
f = fPlay(2surfidle)
f = fPlayTime(surfidle,5)
f = fPlayReverse(2surfidle)

;mermaid submerged behavior sets
[f\BehaviorSet\bSubswim]
f = fWalk(level, 0)

[f\BehaviorSet\bRisesurf]
f = fWalk(surface, 0)

[f\BehaviorSet\bRise]
f = fWalk(rise, 0)

[f\BehaviorSet\bSubdive]
f = fWalk(dive, 0)

[f\BehaviorSet\bSubfloordive]
f = fWalk(floor, 0)

[f\BehaviorSet\bSubspin]
f = fMove(level, 0, subspin, 75)

[f\BehaviorSet\bSubsing]
f = fPlay(2subidle)
f = fPlayWithSound(submerged_sing,subsing)
f = fPlayReverse(2subidle)

[f\BehaviorSet\bSubrest]
f = fPlay(2subidle)
f = fPlayTime(subidle,7)
f = fPlayReverse(2subidle)

[f\BehaviorSet\bMermtrans]
f = fPlayWithSound(merm_appear,mermtrans)

[f\BehaviorSet\bSleep]
f = fPlaySetTank(bSurfsleep, bSubsleep)

[f\BehaviorSet\bSurfsleep]
f = fPlay(2surfidle)
f = fPlay(surfidle)
f = fPlayTime(surfidle,9)
f = fPlay(2surfidle)

[f\BehaviorSet\bSubsleep]
f = fPlay(2subidle)
f = fPlay(subidle)
f = fPlayTime(subidle,9)
f = fPlayReverse(2subidle)

[f\BehaviorSet\bFindKeeperFood]
f = fWalk(keeperfood,0)
f = fPlaySet(bEat)

[f\BehaviorSet\bEat]
f = fFaceTowardFood()
f = fPlay(2subidle)
f = fPlay(submerged_eat)
f = fPlayTime(submerged_eat,9)
f = fPlayReverse(2subidle)

[f\BehaviorSet\bSick]
f = fPlaySetTank(bSurfsick, bSubsick)

[f\BehaviorSet\bSurfsick]
f = fPlay(2surfidle)
f = fPlay(surfidle)
f = fPlayTime(surfidle,9)

[f\BehaviorSet\bSubsick]
f = fPlay(2subidle)
f = fPlay(subidle)
f = fPlayTime(subidle,9)
f = fPlayReverse(2subidle)

[f\BehaviorSet\bDie]
f = fPlaySetTank(bSurfdie, bSubdie)

[f\BehaviorSet\bSurfdie]
f = fPlay(2surfidle)
f = fPlay(surfidle)
f = fPlayTime(surfidle,5)
f = fDie()

[f\BehaviorSet\bSubdie]
f = fPlay(2subidle)
f = fPlay(subidle)
f = fPlayTime(subidle,5)
f = fDie()

[f\BehaviorSet\bWalk]
f = fWalk(0,0)

[f\BehaviorSet\bRun]
f = fRun(0,0)

[f\BehaviorSet\bRattle]
f = fPlayWithSound(box_used,placesound)

[f\BehaviorSet\bChasePrey]
f = fRun(prey,0)

[f\BehaviorSet\bCaughtGuest]
; the time of animation below should match bCaughtBy in guest
