;Bowhead Whale male underwater behavior set probabilities
[m\BehaviorSet\bHappyU]
f = fPlaySetProb(bSubswim,60,bSubtosurf,20,bRise,20)

[m\BehaviorSet\bFriendlyU]
f = fPlaySetProb(bSubtosurf,40,bRise,10,bSubswim,50)

[m\BehaviorSet\bCalmU]
f = fPlaySetProb(bRise,40,bDive,40,bSubswim,20)

[m\BehaviorSet\bNeutralU]
f = fPlaySetProb(bSubswim,50,bRise,20,bDive,30)

[m\BehaviorSet\bUneasyU]
f = fPlaySetProb(bSubswim,40,bFloor,30,bRise,10,bDive,20)

[m\BehaviorSet\bTenseU]
f = fPlaySetProb(bSubswim,30,bFloor,40,bDive,10,bThrash,20)

[m\BehaviorSet\bAngryU]
f = fPlaySetProb(bFloor,50,bSubswim,20,bThrash,20,bDive,10)

;Bowhead Whale male underwater behavior sets
[m\BehaviorSet\bSubswim]
f = fWalk(level, 0)

[m\BehaviorSet\bRise]
f = fWalk(rise, 0)

[m\BehaviorSet\bDive]
f = fWalk(dive, 0)

[m\BehaviorSet\bSubtosurf]
f = fWalk(surface, 0)

[m\BehaviorSet\bFloor]
f = fWalk(floor, 0)

[m\BehaviorSet\bThrash]
f = fPlay(submerged_thrash)

[m\BehaviorSet\bRest]
f = fPlayTime(submerged_rest,3)

;Bowhead male surface behavior set probabilities
[m\BehaviorSet\bHappyS]
f = fPlaySetProb(bSurfswim,40,bSurfslowswim,20,bSurfdive,10,bSurfloor,10,bSurfrest,10,bBreach,10)

[m\BehaviorSet\bFriendlyS]
f = fPlaySetProb(bSurfswim,60,bSurfslowswim,10,bSurfdive,10,bSurfloor,5,bSurfrest,10,bBreach,5)

[m\BehaviorSet\bCalmS]
f = fPlaySetProb(bSurfrest,20,bSurfswim,50,bSurfslowswim,10,bSurfdive,10,bSurfloor,10)

[m\BehaviorSet\bNeutralS]
f = fPlaySetProb(bSurfslowswim,10,bSurfdive,70,bSurfrest,20)

[m\BehaviorSet\bUneasyS]
f = fPlaySetProb(bSurfslowswim,40,bSurfdive,30,bSurfrest,30)

[m\BehaviorSet\bTenseS]
f = fPlaySetProb(bSurfdive,50,bSurfloor,20,bSurfsplash,30)

[m\BehaviorSet\bAngryS]
f = fPlaySetProb(bSurfdive,40,bSurfloor,30,bSurfswim,30)

