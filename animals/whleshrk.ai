f = fWalk(0,0)

[y\BehaviorSet\bDie]
f = fPlaySetTank(bSurfdie, bSubdie)

[y\BehaviorSet\bSurfdie]
f = fWalk(dive, 0)
f = fPlaySet(bSubdie)

[y\BehaviorSet\bSubdie]
f = fHalt()
f = fPlay(2_submerged_sick)
f = fPlay(submerged_sick)
f = fPlayTime(submerged_sick,5)
f = fDie()

[y\BehaviorSet\bRattle]
f = fPlayWithSound(box_used,placesound)

[y\BehaviorSet\bChasePrey]
f = fRun(prey,0)

[y\BehaviorSet\bCaughtGuest]
; the time of animation below should match bCaughtBy in guest
f = fPlaySetTerrain(bcgland,bcgwater,bcgsurf,bcgsub)

[y\BehaviorSet\bcgland]
f = fPlayTime(stand,4)

[y\BehaviorSet\bcgwater]
f = fPlayTime(surfidle,4)

[y\BehaviorSet\bcgsurf]
f = fPlayTime(surfidle,4)

[y\BehaviorSet\bcgsub]
f = fPlayTime(subidle,4)

[y\BehaviorSet\bRunFromPredator]
f = fRun(0,0)

[y\BehaviorSet\bCaughtPrey]
f = fDustBall()
f = fPlaySetTank(bAcsurf,bAcsub)

[y\BehaviorSet\bAcsurf]
f = fPlay(surfidle)

[y\BehaviorSet\bAcsub]
f = fPlay(subidle)

