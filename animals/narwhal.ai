;Beluga male surface behavior sets
[m\BehaviorSet\bSurfswim]
f = fWalk(surface, 0)

[m\BehaviorSet\bSurfdive]
f = fWalk(dive, 0)

[m\BehaviorSet\bSurfloor]
f = fWalk(floor, 0)

[m\BehaviorSet\bSurfvoc]
f = fPlay(surface_2_stop)
f = fPlay(surface_stop)
f = fPlayWithSound(surface_vocalization,surfvocl)
f = fPlay(surface_stop)
f = fPlayReverse(surface_2_stop)

[m\BehaviorSet\bSurfhorn]
f = fMove(surface, 0, surface_horn, 33)

[m\BehaviorSet\bSurfstp]
f = fPlay(surface_2_stop)
f = fPlayTime(surface_stop,5)
f = fPlayReverse(surface_2_stop)

[m\BehaviorSet\bSurfspin]
f = fMoveWithSound(surface, 0, surface_spin, 33, surfspin)
f = fPlay(surfidle)

;Bottlenose dolphin male submerged behavior sets
[m\BehaviorSet\bSubswim]
f = fWalk(level, 0)

[m\BehaviorSet\bRisesurf]
f = fWalk(surface, 0)

[m\BehaviorSet\bRise]
f = fWalk(rise, 0)

[m\BehaviorSet\bSubdive]
f = fWalk(dive, 0)

[m\BehaviorSet\bSubfloordive]
f = fWalk(floor, 0)

[m\BehaviorSet\bSubroll]
f = fMove(level, 0, submerged_roll, 33)
f = fPlay(subidle)

[m\BehaviorSet\bSubvoc1]
f = fPlayWithSound(submerged_vocalization_1,subvocl1)

[m\BehaviorSet\bSubvoc2]
f = fPlayWithSound(submerged_vocalization_2,subvocl2)

[m\BehaviorSet\bSubvocs]
f = fPlayWithSound(submerged_vocalization_2,subvocl2)
f = fPlayWithSound(submerged_vocalization_1,subvocl1)

[m\BehaviorSet\bSubleft]
f = fPlay(submerged_2_stop)
f = fPlay(submerged_stop)
f = fPlay(submerged_look_left)

[m\BehaviorSet\bSubright]
f = fPlay(submerged_2_stop)
f = fPlay(submerged_stop)
f = fPlay(submerged_look_right)

[m\BehaviorSet\bSleep]
f = fPlaySetTank(bSurfsleep, bSubsleep)

[m\BehaviorSet\bSurfsleep]
f = fPlay(surface_2_stop)
f = fPlayTime(surface_stop,9)
f = fPlayReverse(surface_2_stop)

[m\BehaviorSet\bSubsleep]
f = fPlay(submerged_2_stop)
f = fPlayTime(submerged_stop,9)
f = fPlayReverse(submerged_stop)

[m\BehaviorSet\bFindKeeperFood]
f = fWalk(keeperfood,0)
f = fPlaySet(bEat)

[m\BehaviorSet\bEat]
f = fPlaySetTank(bSurfeat,bSubeat)

[m\BehaviorSet\bSurfeat]
f = fFaceTowardFood()
f = fPlayTime(surface_eat,9)
f = fPlay(surfidle)

[m\BehaviorSet\bSubeat]
f = fFaceTowardFood()
f = fPlayTime(submerged_eat,9)
f = fPlay(subidle)

[m\BehaviorSet\bSick]
f = fPlaySetTank(bSurfsick,bSubsick)

[m\BehaviorSet\bSurfsick]
f = fPlay(surface_2_sick)
f = fPlayTime(surface_sick,9)
f = fPlayReverse(surface_2_sick)

[m\BehaviorSet\bSubsick]
f = fPlay(submerged_2_sick)
f = fPlayTime(submerged_sick,9)
f = fPlayReverse(submerged_2_sick)

[m\BehaviorSet\bDie]
