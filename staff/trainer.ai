[BehaviorSet\bWalkRandomly]
f = fWalk(0,0)
f = fWalk(0,0)
f = fWalk(0,0)
f = fWalk(0,0)
f = fWalk(0,0)
f = fPlayTime(idle,2)

[BehaviorSet\bWalk]
f = fPlaySetTerrain(bWalkland,bWalkwater,bWalksurf,bWalksub)

[BehaviorSet\bWalkland]
f = fWalk(0,0)

[BehaviorSet\bWalkwater]
f = fWalk(0,0)

[BehaviorSet\bWalksurf]
f = fMoveWithSound(0,0,surfswim,33,surfswim)

[BehaviorSet\bWalksub]
f = fMoveWithSound(0,0,subswim,33,subswim)

[BehaviorSet\bCleanTankSurf]
f = fMoveWithSound(level, 0, surfclean, 33, surfpoo)

[BehaviorSet\bCleanTankSub]
f = fMoveWithSound(level, 0, subclean, 33, subpoo)

[BehaviorSet\bZooRoutine]
f = fKeeperRoutine()

[BehaviorSet\bFeedTank]
f = fFaceTowardTarget()
f = fPlayWithSound(subfeed,feed_carn)
f = fPlayWithSound(subfeed,feed_carn)
f = fPlayWithSound(subfeed,feed_carn)

[BehaviorSet\bHeal]
f = fFaceTowardTarget()
f = fPlaySetTank(bSurfheal,bSubheal)

[BehaviorSet\bSurfheal]
f = fPlayWithSound(surfheal,heal_animal)
f = fPlayReverse(surfheal)

[BehaviorSet\bSubheal]
f = fPlayWithSound(subheal,heal_animal)
f = fPlayReverse(subheal)

[BehaviorSet\bClean]
f = fFaceTowardTarget()
f = fPlayWithSound(subpoo,subpoo)
f = fPlayWithSound(subpoo,subpoo)
f = fPlay(idle)

[BehaviorSet\bChaseAnimal]
f = fRun(0,0)

[BehaviorSet\bRetrieveAnimal]
f = fRun(0,0)

[BehaviorSet\bRunFromPredator]
f = fPlaySetTerrain(bRunpredland,bRunpredwater,bRunpredsurf,bRunpredsub)

[BehaviorSet\bRunpredland]
f = fRun(0,0)

[BehaviorSet\bRunpredwater]
f = fRun(0,0)

[BehaviorSet\bRunpredsurf]
f = fMoveWithSound(0,0,surfswim,66,surfswim)

[BehaviorSet\bRunpredsub]
f = fMoveWithSound(0,0,subswim,66,subswim)

[BehaviorSet\bClimbUp]
f = fFly(climb, 33)

[BehaviorSet\bClimbDown]
f = fFlyReverse(climb, 33)

[BehaviorSet\bTrickStart]
f = fPlayWithSound(point,point)
f = fPlayWithSound(whistle,blow)

[BehaviorSet\bTrickEnd]
f = fPlayWithSound(point,point)
f = fPlayWithSound(whistle,blow)

[BehaviorSet\bTrickFeed]
f = fPlayWithSound(throw,throw)

